{
  "name": "frontend",
  "version": "0.1.0",
  "homepage": "https://derekmborges.github.io/pairminator",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.121",
    "@mui/material": "^5.11.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "firebase": "^9.17.1",
    "gh-pages": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "emulators": "firebase login firebase emulators:start --debug --project pairminator --token \"$FIREBASE_TOKEN\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -u 'github-actions-bot <support+actions@github.com>'",
    "test": "cypress run",
    "test-ci": "firebase emulators:exec --only auth,firestore \"yarn test\" --token \"$FIREBASE_TOKEN\"",
    "kill-ui": "ps augx | grep -v grep | grep \"react-scripts/scripts/start\" | awk '{print $2}' | xargs kill",
    "ci": "concurrently -c \"auto\" \"yarn start\" \"yarn test-ci && yarn kill-ui\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "concurrently": "^8.0.1",
    "cypress": "^12.10.0",
    "firebase-tools": "^11.25.2"
  }
}
